{
    "routes": {
        "home": {
            "path": "/",
            "props": {
                "config": {
                    "parallaxImages": [
                        "/images/parallax/crane/parallax-3-min.png",
                        "/images/parallax/crane/parallax-2-min.png",
                        "/images/parallax/crane/parallax-1-min.png",
                        "/images/parallax/crane/parallax-0-min.png"
                    ],
                    "slider": {
                        "index": 1,
                        "slides": [
                            {"id": 1, "src": "/images/ref/slider/1.jpeg", "header": "Aurum Family hotel", "text": "Hajdúszoboszló, 2012"},
                            {"id": 2, "src": "/images/ref/slider/2.jpeg", "header": "Családi ház", "text": "Hajdúszoboszló, 2015"},
