                            {"id": 4, "src": "/images/ref/slider/4.jpeg", "header": "Szénatároló", "text": "Hajdúböszörmény, 2013"},
                            {"id": 5, "src": "/images/ref/slider/5.jpeg", "header": "Családi ház", "text": "Debrecen, 2018"},
                            {"id": 6, "src": "/images/ref/slider/6.jpeg", "header": "Családi ház", "text": "Hajdúszoboszló, 2019"},
                            {"id": 7, "src": "/images/ref/slider/7.jpeg", "header": "10 lakásos társasház", "text": "Hajdúszoboszló, 2018"},
                            {"id": 8, "src": "/images/ref/slider/8.jpeg", "header": "Mátyás hotel", "text": "Hajdúszoboszló, 2014"},
                            {"id": 9, "src": "/images/ref/slider/9.jpeg", "header": "Közösségi ház", "text": "Nyírlugos, 2015"},
                            {"id": 10, "src": "/images/ref/slider/10.jpeg", "header": "Nyomdaipari épület", "text": "Debrecen, 2013"},
                            {"id": 11, "src": "/images/ref/slider/11.jpeg", "header": "Platán hotel", "text": "Hajdúszoboszló, 2016"},
                            {"id": 12, "src": "/images/ref/slider/12.jpeg", "header": "Angyal borászat és panzió", "text": "Rátka, 2015"},
                            {"id": 13, "src": "/images/ref/slider/13.jpeg", "header": "31 lakásos társasház", "text": "Oasis Residence, Hajdúszoboszló, 2020"}
                        ],
                        "controls": {
                            "dots": false
                        }
                    },
                    "partners": [
                        { "name": "Generálép-3 Kft.", "href": "//generalep-3.hu", "logo": "/images/partners/generalep-3.svg"},
                        { "name": "Chemostart Kft.", "href": "//chemostart.hu", "logo": "/images/partners/chemo.png"},
                        { "name": "GSV Gulyás Tüzép Kft.", "href": "//gsv.hu", "logo": "/images/partners/gsv.png"},
                        { "name": "Dutrade Zrt.", "href": "//dutrade.hu", "logo": "/images/partners/dutrade.png"},
                        { "name": "Nord-Point Kft.", "href": "//nord-point.hu", "logo": "/images/partners/nord-point2.png"},
                        { "name": "Zákány Szerszámház Kft.", "href": "//zakanyszerszamhaz.hu", "logo": "/images/partners/zakany.png"},
                        { "name": "RR.Westor Kft.", "href": "//rrwestor.hu", "logo": "/images/partners/rr-westor.png"},
                        { "name": "Magyar Doka Zsalutechnika Kft.", "href": "//doka.hu", "logo": "/images/partners/doka.svg"}
                    ]
                }
            }
        },
        "services": {
            "path": "/services"
        },
        "contact": {
            "path": "/contact"
        },
        "portfolio": {
            "path": "/portfolio"
        },
        "partners": {
            "path": "/partners"
        },
        "notFound": {
            "path": "*"
        },
        "defaults": {
            "props": {
                "config": {
                    "animations": {
                        "in": {
                            "duration": 300,
                            "delay": 300
                        },
                        "out": {
                            "duration": 300
                        }
                    }
                }
            }
        }
    },
    "menus": [
        {
            "route": "home",
            "target": "_self",
            "label": "menu.home"
        },
        {
            "route": "services",
            "target": "_self",
            "label": "menu.services"
        },
        {
            "route": "portfolio",
            "target": "_self",
            "label": "menu.portfolio"
        },
        {
            "route": "partners",
            "target": "_self",
            "label": "menu.partners"
        },
        {
            "route": "contact",
            "target": "_self",
            "label": "menu.contact"
        }
    ],
    "footer": {
        "copyrightLabel": "copyright",
        "socials": [
            { "icon": "facebook", "href": "//facebook.com" },
            { "icon": "envelope", "href": "mailto://info@pusit.hu" },
            { "icon": "instagram", "href": "//instagram.com" },
            { "icon": "pinterest", "href": "//pinterest.com" }
        ],
        "article": "/articles/footer.md",
        "legals": [
            {"label": "privacy", "href": "#"},
            {"label": "dataForwarding", "href": "#"},
            {"label": "dataProtection", "href": "#"}
        ],
        "links": [
            {"label": "footer.links.home", "href": "/"},
            {"label": "footer.links.about", "href": "/about"},
            {"label": "footer.links.portfolio", "href": "/portfolio"},
            {"label": "footer.links.partners", "href": "/partners"},
            {"label": "footer.links.contact", "href": "/contact"}
        ]
    },
    "i18n": {
        "languages": {
            "en": "/i18n/en.json",
            "hu": "/i18n/hu.json"
        },
        "default": "hu"
    },
    "animations": {
        "defaults": {
            "in": {
                "duration": 300,
                "delay": 300
            },
            "out": {
                "duration": 300,
                "delay": 300
            }
        }
    },
    "mediaqueries": {
        "xs": "screen and (max-width: 575px)",
        "sm": "screen and (min-width: 576px)",
        "md": "screen and (min-width: 768px)",
        "lg": "screen and (min-width: 992px)",
        "xl": "screen and (min-width: 1200px)"
    }
}